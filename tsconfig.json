{
  "compilerOptions": {
    "target": "es6",
    "resolveJsonModule": true,
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "baseUrl": "./",
  "include": [
    "src/swagger-output.json",
    "./**/*",
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}